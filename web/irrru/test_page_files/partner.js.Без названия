function sendHitApiRequests(spUData){if(typeof spUData==='undefined'){spUData={updateSpUIDPartnerDomain:spApi.storageData('updateSpUIDPartnerDomain'),spUserIDCreated:'false',updateSpUID:'false'};var spUID=getSpUIDFromLocalStorage();var globalId=spApi.getGlobalUserIdFromLocalStorage();var terminationCondition=spApi.globalUserIdIsDisabled?spUID===null:spUID===null||globalId===null;if(terminationCondition){return false;}

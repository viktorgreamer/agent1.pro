x: real-estate\/commercial-sale\/offices\/ *\/\r\n        var category = sQuery('#categoryLink').val();\r\n\r\n        \/* This is global variable *\/\r\n        \/\/var regionURI = rootRegionURI;\r\n        var regionURI = '';\r\n\r\n        var region = sQuery('#full_address_string').val() || '';\r\n\r\n        \/* This is old version of region\r\n         if (sQuery('[name=\"region\"]').exists()) {\r\n         region = sQuery('[name=\"region\"]').attr('data-uri').replace('.irr.ru\/', '').replace('https:\/\/', '').replace('http:\/\/', '') + '\/';\r\n         }\r\n         *\/\r\n\r\n        var filters = '';\r\n        var categoryTab = sQuery('.tabs__link_active.js-categoryTab');\r\n        var productGrid = sQuery('.listing.js-productGrid a:first');\r\n\r\n        if (categoryTab.exists() && categoryTab.attr('href').indexOf('\/search\/') > -1) {\r\n            filters = ((categoryTab.attr('href') || '').split('\/search\/') || '').pop();\r\n        } else if (productGrid.exists() && productGrid.attr('href').indexOf('\/search\/') > -1) {\r\n            filters = ((productGrid.attr('href') || '').split('\/search\/') || '').pop();\r\n        } else if (window.location.href.indexOf('\/search\/') > -1) {\r\n            filters = window.location.href.split('\/search\/')[1];\r\n        }\r\n\r\n        return (apiUrl + 'category=' + category + '&region=' + regionURI + region + '&filters=\/search\/' + encodeURIComponent(filters));\r\n\r\n    };\r\n\r\n    \/**\r\n     * Save filter to cookie\r\n     *\r\n     * @param filter {string}\r\n     * *\/\r\n    self.saveFilterToStorage = function(filter) {\r\n        var subscribedFilters = [];\r\n\r\n        if (spApi.localStorageGet('insSubscribedFilters') !== null) {\r\n            subscribedFilters = JSON.parse(spApi.localStorageGet('insSubscribedFilters'));\r\n        }\r\n\r\n        var exists = subscribedFilters.filter(function(v) {\r\n            return v == filter\r\n        }).length > 0;\r\n\r\n        if (!exists) 